{
   "name": "barnes-craftar-capture",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "axios": "^0.18.0",
      "dotenv": "^6.2.0",
      "elasticsearch": "^15.3.1",
      "express": "^4.16.4",
      "multer": "^1.4.5-lts.1",
      "serverless-http": "^2.7.0",
      "vuforia-api": "^0.3.2"
   },
   "engines": {
      "node": ">=0.10"
   },
   "scripts": {
      "start": "PORT=3004 react-scripts start",
      "build": "rm -rf dist; react-scripts build; npm run build-server",
      "build-server": "tsc -p server/",
      "postinstall": "npx patch-package", 
      "postbuild": "cp -r build dist && rm -rf build",
      "test": "react-scripts test --env=jsdom",
      "eject": "react-scripts eject",
      "serve": "npm run build-server && node dist/index.js",
      "deploy:dev": "serverless deploy -v --stage dev",
      "deploy:prod": "serverless deploy -v --stage prod",
      "watch-server": "tsc -w -p server/",
      "local-server": "nodemon --delay 500ms dist/server/index.js",
      "dev": "concurrently \"npm run watch-server\" \"npm run local-server\""
   },
   "devDependencies": {
      "@material-ui/core": "^3.7.1",
      "@types/axios": "^0.14.0",
      "@types/dotenv": "^6.1.0",
      "@types/elasticsearch": "^5.0.30",
      "@types/express": "^4.16.0",
      "@types/jest": "^23.3.12",
      "@types/material-ui": "^0.21.5",
      "@types/multer": "^1.4.7",
      "@types/node": "^10.17.60",
      "@types/react": "^16.7.18",
      "@types/react-dom": "^16.0.11",
      "@types/react-redux": "^6.0.20",
      "@types/react-router-dom": "^4.3.1",
      "@typescript-eslint/eslint-plugin": "^5.59.11",
      "@typescript-eslint/parser": "^5.59.11",
      "blueimp-load-image": "^2.20.1",
      "concurrently": "^7.0.0",
      "cropperjs": "^1.5.12",
      "eslint": "^8.43.0",
      "eslint-config-prettier": "^8.8.0",
      "eslint-plugin-import": "^2.27.5",
      "eslint-plugin-prettier": "^4.2.1",
      "node-sass": "^4.10.0",
      "nodemon": "^2.0.22",
      "prettier": "^2.8.8",
      "react": "^16.14.0",
      "react-cropper": "^2.1.8",
      "react-dom": "^16.14.0",
      "react-html5-camera-photo": "^1.2.9",
      "react-redux": "^6.0.0",
      "react-router-dom": "^4.3.1",
      "react-scripts": "^2.1.3",
      "redux": "^4.0.1",
      "typeface-roboto": "0.0.54",
      "typescript": "^4.5.5",
      "webpack-dev-server": "^3.1.14"
   },
   "browserslist": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
   ],
   "proxy": "http://localhost:4004"
}
